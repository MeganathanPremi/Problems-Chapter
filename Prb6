import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner (System.in);
        System.out.print("Enter the X value :");
        int x = scan.nextInt();
        int i=1;
            while(x*i<=1000){
                System.out.println(x*i);
                i++;
            }
        
        
    }
}
