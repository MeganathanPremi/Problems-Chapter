class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter the number: ");
        int n = scan.nextInt();

        for (int i = 0; i<n; i++) {
            for (int j = n - i; j >= 1; j--) {
                System.out.print(j);
            }
            System.out.println();
        }
    }
}
