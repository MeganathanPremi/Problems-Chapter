https://www.geeksforgeeks.org/problems/middle-of-three2926/1

class Solution {
    int middle(int a, int b, int c) {
        // code here
        int min_val = Math.min(a,Math.min(b,c));
        int max_val = Math.max(a,Math.max(b,c));
        
        if(a!=min_val && a!=max_val){
            return a;        
        }
        else if (b!=min_val && b!=max_val){
            return b;
        }
        else{
            return c;
        }
    }
}
