https://www.geeksforgeeks.org/problems/special-series-sum0903/1

class Solution {
    static long sumOfTheSeries(long n) {
        // code here
        long sum=0,seriesTerm=0;
        for (int i=1;i<=n;i++){
            sum = sum+i;
            seriesTerm = seriesTerm + sum;
        }
        return seriesTerm;
        
    }
}
